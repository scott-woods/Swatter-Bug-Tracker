@{ 
    ViewBag.Title = "Swatter - Role Management";
}
@model BugTracker.Models.
<div class="container-fluid">
    <div class="row">
        <div class="col">
            <h1 class="mt-4">Manage User Roles</h1>
            <div class="card mb-4">
                <div class="card-header">Users</div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Role</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col">

        </div>
    </div>
</div>